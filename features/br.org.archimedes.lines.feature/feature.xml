      All archimedes is GPL v.2 licensed.
   </license>

   <url>
      <update label="Archimedes Update" url="http://update.archimedes.org.br/site/"/>
   </url>

   <includes
         id="br.org.archimedes.core.feature"
         version="0.0.0"/>

   <requires>
      <import plugin="br.org.archimedes.core"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="br.org.archimedes.undo"/>
   </requires>

   <plugin
         id="br.org.archimedes.line"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="br.org.archimedes.infiniteline"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="br.org.archimedes.polyline"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="br.org.archimedes.polyline.explode"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="br.org.archimedes.polyline.rectangle"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="br.org.archimedes.semiline"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
